using System;

namespace HomeWork
{
    class Program
    {
        static void Main(string[] args)
        {
            int imagesCount = 52;
            int imagesInOneRow = 3;

            int fullRows = imagesCount / imagesInOneRow;
            int remainImages = imagesCount % imagesInOneRow;

            Console.WriteLine($"Кол-во заполненных рядов: {fullRows}\n" + $"Кол-во картинок сверх меры: {remainImages}");
            Console.ReadKey();
        }
    }
}

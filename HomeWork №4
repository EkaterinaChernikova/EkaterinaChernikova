using System;

namespace HomeWork
{
    class Program
    {
        static void Main(string[] args)
        {
            int paintings = 52;
            int rowsOfPaintings = 3;

            int filledRowsOfPaintings = paintings / rowsOfPaintings;
            int remains = paintings % rowsOfPaintings;

            Console.WriteLine($"Кол-во заполненных рядов: {filledRowsOfPaintings}\n" + $"Кол-во картинок сверх меры: {remains}");
            Console.ReadKey();
        }
    }
}

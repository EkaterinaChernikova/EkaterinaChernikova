using System;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            int gallerySize = 52;

            int lineCount = gallerySize / 3;
            int remains = gallerySize % 3;

            Console.WriteLine($"Кол-во полных строк: {lineCount}\n" +
                $"Кол-во картинок в не полной строке: {remains}");
            Console.ReadKey();
        }
    }
}

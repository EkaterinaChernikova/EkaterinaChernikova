using System;

namespace HomeWork
{
    class Program
    {
        static void Main(string[] args)
        {
            int firstNumber = 5;
            int lastNumber = 96;
            int numberStep = 7;

            //В for обычно задается начальное и конечное значение переменной цикла и какие-либо правила для её изменения.Тут for подходит
            for (int i = firstNumber; i <= lastNumber; i += numberStep)
            {
                Console.WriteLine(i);
            }
            Console.ReadKey();
        }
    }
}

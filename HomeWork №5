using System;

namespace HomeWork
{
    class Program
    {
        static void Main(string[] args)
        {
            string firstName;
            string lastName;
            string buffer;

            firstName = "Вазовский";
            lastName = "Майк";
            
            Console.WriteLine($"До: Имя: {firstName} Фамилия: {lastName}");
            
            buffer = firstName;
            firstName = lastName;
            lastName = buffer;
            
            Console.WriteLine($"После: Имя: {firstName} Фамилия: {lastName}");
            Console.ReadKey();
        }
    }
}

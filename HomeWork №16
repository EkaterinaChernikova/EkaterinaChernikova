using System;

namespace HomeWork
{
    class Program
    {
        static void Main(string[] args)
        {
            Random random = new Random();
            int number;
            int minNumber = 1;
            int maxNumber = 28;
            int divisibleNumber = 0;
            int minDivisible = 100;
            int maxDivisible = 1000;

            number = random.Next(minNumber, maxNumber);

            for (int i = minDivisible; i < maxDivisible; i++)
            {
                for(int j = i; j > 0;)
                {
                    j -= number;

                    if (j == 0)
                        divisibleNumber++;
                }
            }

            Console.WriteLine($"Кол-во трехзначных чисел кратных {number}: {divisibleNumber}");
            Console.ReadKey();
        }
    }
}

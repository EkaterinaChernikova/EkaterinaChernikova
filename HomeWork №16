using System;

namespace HomeWork
{
    class Program
    {
        static void Main(string[] args)
        {
            Random random = new Random();
            int N;
            int minN = 1;
            int maxN = 28;
            int divisibleN = 0;
            int minDivisible = 100;
            int maxDivisible = 1000;
            double divisible;

            N = random.Next(minN, maxN);

            for(double i = minDivisible; i < maxDivisible; i++)
            {
                divisible = Math.IEEERemainder(i, N);
                //divisible = Math.Sqrt(i);
                if (divisible == 0)
                    divisibleN++;
            }

            Console.WriteLine($"Кол-во трехзначных чисел краттных {N}: {divisibleN}");
            Console.ReadKey();
        }
    }
}
